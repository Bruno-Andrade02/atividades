tices:\n","      g.node(i.conteudo,i.conteudo,fontsize=\"10\")\n","    for k,v in self.listaAdjacentes.items():\n","      for j in v:\n","        g.edge(k,j,dir=\"none\")\n","    return g"],"metadata":{"id":"09T4zPbu3-Pv"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["gr = Grafo()\n","\n","gr.adicionaVertice('A')\n","gr.adicionaVertice('B')\n","gr.adicionaVertice('C')\n","gr.adicionaVertice('D')\n","gr.adicionaVertice('E')\n","gr.adicionaVertice('F')\n","gr.adicionaVertice('G')\n","gr.adicionaVertice('H')\n","gr.adicionaVertice('I')\n","\n","gr.addConexao('A','B')\n","gr.addConexao('A','C')\n","gr.addConexao('A','D')\n","gr.addConexao('B','E')\n","gr.addConexao('B','F')\n","gr.addConexao('B','G')\n","gr.addConexao('C','G')\n","gr.addConexao('C','H')\n","gr.addConexao('D','I')\n","\n"],"metadata":{"id":"P4TYIklZ8Dq0"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["gr.desenhoGrafo()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":272},"id":"DcGCu4qq9c6W","executionInfo":{"status":"ok","timestamp":1661263113627,"user_tz":180,"elapsed":329,"user":{"displayName":"Bruno Andrade","userId":"02416081201046415276"}},"outputId":"247764a9-4db7-4fe4-9e1e-4d22f5d6baae"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["<graphviz.dot.Graph at 0x7ff883adef10>"],"image/svg+xml":"<?xml version=\"1.0\" encoding=\"UTF-8\" standalone=\"no\"?>\n<!DOCTYPE svg PUBLIC \"-//W3C//DTD SVG 1.1//EN\"\n \"http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd\">\n<!-- Generated by graphviz version 2.40.1 (20161225.0304)\n -->\n<!-- Title: %3 Pages: 1 -->\n<svg width=\"350pt\" height=\"188pt\"\n viewBox=\"0.00 0.00 350.00 188.00\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\">\n<g id=\"graph0\" class=\"graph\" transform=\"scale(1 1) rotate(0) translate(4 184)\">\n<title>%3</title>\n<polygon fill=\"#ffffff\" stroke=\"transparent\" points=\"-4,4 -4,-184 346,-184 346,4 