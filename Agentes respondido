  self.periodoAnalise = periodoAnalise\n","        self.periodoPrevisao = periodoPrevisao\n","        self.saldoReal = saldoR\n","        self.saldoDolar = saldoD\n","        self.saldoBolsa = saldoB\n","        self.evolucaoReal = [saldoR]\n","        self.precoMedioAcao = precoAcao\n","    \n","    def atualizaEstado(self,valorDolar,indiceBolsa):\n","        self.observacao +=1\n","        self.precoMedioAcao *= (1+indiceBolsa/100)\n","        self.estado.append((valorDolar,self.precoMedioAcao))\n","\n","    def percepcao(self,dolar,indBolsa):\n","        self.atualizaEstado(dolar,indBolsa)   \n","    #\n","    # o que aconteceria se ....\n","    #\n","    def simularInvestimentoDolar(self):\n","        if self.saldoReal>0:\n","            return self.saldoReal/self.estado[-1][0]\n","        else:\n","            return 0\n","    \n","    def simularInvestimentoBolsa(self):\n","        if self.saldoReal>0:\n","            return self.saldoReal/self.estado[-1][1]\n","        else:\n","            return 0\n","        \n","    def previsao(self,serie,periodoAnterior,periodoAvante): # Janela de previsao\n","        if len(serie)<2:\n","            return serie[-1]\n","        if len(serie)<periodoAnterior:\n","            periodoAnterior = len(serie)\n","        serieEfetiva = serie[-periodoAnterior:]\n","        periodos = [i for i in range(len(serieEfetiva))]\n","        a,b = regressaoLinear(periodos,serieEfetiva)\n","        diaPrevisao = periodos[-1]+periodoAvante        \n","        return a * diaPrevisao + b   \n","        \n","    def lucroEsperadoDolar(self,periodoAnterior,periodoFuturo):\n","        valorAnterior = self.saldoReal\n","        totalDolar = self.simularInvestimentoDolar()\n","        while True:\n","            valorFuturoDolar = self.previsao([i[0] for i in self.estado],periodoAnterior,periodoFuturo)\n","            if valorFuturoDolar>0:\n","                break\n","            else:\n","                periodoFuturo -=1\n","        valorAtual = valorFuturoDolar * totalDolar\n","        return valorAtual-valorAnterior\n","     \n","    def lucroEsperadoBolsa(self,periodoAnterior,periodoFuturo):\n","        valorAnterior = self.saldoReal\n","        totalBolsa = self.simularInvestimentoBolsa()\n","        for i in range(1,periodoFuturo+1):\n","            valorFuturoBolsa = self.previsao([i[1] for i in self.estado],periodoAnterior,i)\n","            if valorFuturoBolsa>0:\n","                break\n","            else:\n","                periodoFuturo -=1\n","        valorAtual = valorFuturoBolsa * totalBolsa\n","        return valorAtual - valorAnterior\n","        \n","    def saida(self):\n","        #\n","        previsaoLucroDolar = self.lucroEsperadoDolar(self.periodoAnalise,self.periodoPrevisao)\n","        previsaoLucroBolsa = self.lucroEsperadoBolsa(self.periodoAnalise,self.periodoPrevisao)\n","        #comprar dolar ou investir na bolsa????\n","        return self.funcaoAgente(previsaoLucroDolar,previsaoLucroBolsa,\n","                                 self.saldoReal,self.saldoBolsa,self.saldoDolar)\n","    \n","    def comprarDolar(self): #Ação comprar \n","        if self.saldoReal>0:\n","            self.saldoDolar += self.saldoReal/self.estado[-1][0]\n","            self.saldoReal = 0\n","            \n","            \n","    def venderDolar(self): #Ação vender\n","        if self.saldoDolar>0:\n","            self.saldoReal += self.saldoDolar * self.estado[-1][0]\n","            self.saldoDolar = 0\n","        self.evolucaoReal.append(self.saldoReal)\n","        \n","    def comprarAcao(self):\n","        if self.saldoReal>0:\n","            self.saldoBolsa += self.saldoReal/self.estado[-1][1] #considerando valor em ações\n","            self.saldoReal = 0\n","    \n","    def venderAcao(self):\n","        if self.saldoBolsa>0:\n","            self.saldoReal += self.saldoBolsa * self.estado[-1][1]\n","            self.saldoBolsa = 0\n","    \n","    def desempenho(self): #lucrototal\n","        return self.evolucaoReal[-1] - self.evolucaoReal[0]\n","          "]},{"cell_type":"code","execution_count":null,"metadata":{"id":"ZslEt6Z2mfLx"},"outputs":[],"source":["def funcaoAgenteInvestidor(lucroEsperadoDolar,lucroEsperadoBolsa,saldoR,saldoB,saldoD):\n","    if saldoR>0:\n","        if lucroEsperadoDolar >= lucroEsperadoBolsa:\n","            return \"comprarDolar()\"\n","        else:\n","            return \"comprarAcao()\"\n","    else:\n","        if saldoB>0:\n","            return \"venderAcao()\"\n","        else:\n","            return \"venderDolar()\"\n","    \n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"wsDrx2uCmfLx"},"outputs":[],"source":["agenteInvestidor = Investidor([],funcaoAgenteInvestidor,saldoR=1000,precoAcao=10)"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"JE2M9Pv2mfLx"},"outputs":[],"source":["class AmbienteMercadoFinanceiro(Ambiente):\n","    \n","    def __init__(self,estadoInicial):\n","        super().__init__(estadoInicial)\n","    \n","    def percepcao(self, agente):\n","        for i in self.estado:\n","            for ag in agentes:\n","                ag.percepcao(i)\n","        return\n","    \n","    def executaAmbiente(self):\n","        #print(\"Iniciando processo \",self.estado)\n","        for gst in self.estado:\n","            for ag in self.agentes:\n","                ag.percepcao(gst[0],gst[1])\n","                atuacao = ag.saida()\n","                atuacao = \"ag.\"+atuacao\n","                eval(atuacao) # Executa a função correspondente\n","                \n","    def desemPenhoAgentes(self):\n","        desempenho=[]\n","        for i,ag in enumerate(self.agentes):\n","            desempenho.append((i,ag.desempenho()))\n","        return desempenho          "]},{"cell_type":"code","execution_count":null,"metadata":{"id":"gbjy8T7fmfLx"},"outputs":[],"source":["estadoInicial = [(i,j) for i,j in zip(list(reversed(serieHistoricaDolar)),ibovespa)]\n","mercado = A